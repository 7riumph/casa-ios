"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Button = _interopRequireDefault(require("../src/components/Button"));
describe('Button', function () {
  it('should render correctly', function () {
    var mockOnPress = jest.fn();
    var _render = (0, _reactNative.render)(_react.default.createElement(_Button.default, {
        title: "Test",
        onPress: mockOnPress
      })),
      getByText = _render.getByText;
    expect(getByText('Test')).toBeTruthy();
  });
  it('should call onPress when pressed', function () {
    var mockOnPress = jest.fn();
    var _render2 = (0, _reactNative.render)(_react.default.createElement(_Button.default, {
        title: "Test",
        onPress: mockOnPress
      })),
      getByText = _render2.getByText;
    _reactNative.fireEvent.press(getByText('Test'), {});
    expect(mockOnPress).toHaveBeenCalledTimes(1);
  });
});
